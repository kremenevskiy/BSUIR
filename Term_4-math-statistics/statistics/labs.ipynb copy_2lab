    "a_Y = Y(a)\n",
    "b_Y = Y(b)\n",
    "print(f'New interval: {(a_Y, b_Y)}')\n",
    "\n",
    "\n",
    "def F(y):\n",
    "    if y < a_Y: return 0\n",
    "    if a_Y <= y < b_Y: return 4 / np.pi * np.arctan(y)\n",
    "    if y > b_Y: return 1\n",
    "\n",
    "    \n",
    "def f(y):\n",
    "    if a_Y < y < b_Y: return 4 / (np.pi * (1 + y**2))\n",
    "    return 0"
   ]
